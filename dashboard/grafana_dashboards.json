{
  "dashboard": {
    "title": "QuantumEdge Pipeline Performance",
    "tags": ["quantum", "pipeline", "performance"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "type": "piechart",
        "title": "Routing Decisions",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "postgres-datasource"
            },
            "rawSql": "SELECT \n  solver_type as metric, \n  COUNT(*) as value\nFROM jobs\nWHERE timestamp >= NOW() - INTERVAL '24 hours'\nGROUP BY solver_type\nORDER BY value DESC;",
            "format": "table"
          }
        ],
        "options": {
          "pieType": "pie",
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "tooltip": {
            "mode": "single"
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "tooltip": false,
                "viz": false,
                "legend": false
              }
            },
            "mappings": [
              {
                "type": "value",
                "options": {
                  "quantum": {
                    "color": "blue",
                    "index": 0
                  },
                  "classical": {
                    "color": "green",
                    "index": 1
                  }
                }
              }
            ]
          }
        }
      },
      {
        "id": 2,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "type": "timeseries",
        "title": "Execution Time Comparison",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "postgres-datasource"
            },
            "rawSql": "SELECT \n  timestamp as time,\n  solver_type,\n  AVG(execution_time_ms) OVER (\n    PARTITION BY solver_type \n    ORDER BY timestamp \n    ROWS BETWEEN 9 PRECEDING AND CURRENT ROW\n  ) as execution_time_ma\nFROM jobs\nWHERE timestamp >= NOW() - INTERVAL '24 hours'\nORDER BY timestamp;",
            "format": "time_series"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "spanNulls": false
            },
            "unit": "ms"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "classical"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "green"
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "quantum"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "blue"
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
      {
        "id": 3,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "type": "timeseries",
        "title": "Energy Consumption",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "postgres-datasource"
            },
            "rawSql": "SELECT \n  timestamp as time,\n  solver_type,\n  AVG(energy_consumed_j) as energy_consumption\nFROM jobs\nWHERE timestamp >= NOW() - INTERVAL '24 hours'\nGROUP BY timestamp, solver_type\nORDER BY timestamp;",
            "format": "time_series"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20,
              "showPoints": "never"
            },
            "unit": "joule"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "quantum"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "light-blue"
                  }
                },
                {
                  "id": "custom.fillOpacity",
                  "value": 30
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
      {
        "id": 4,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "type": "histogram",
        "title": "Problem Size Distribution",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "postgres-datasource"
            },
            "rawSql": "SELECT \n  problem_size as value\nFROM jobs\nWHERE timestamp >= NOW() - INTERVAL '24 hours'\nORDER BY problem_size;",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "tooltip": false,
                "viz": false,
                "legend": false
              }
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
      {
        "id": 5,
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 16
        },
        "type": "gauge",
        "title": "Solution Quality",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "postgres-datasource"
            },
            "rawSql": "SELECT \n  AVG(solution_quality) as quality\nFROM jobs\nWHERE timestamp >= NOW() - INTERVAL '1 hour';",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.7
                },
                {
                  "color": "green",
                  "value": 0.85
                }
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "orientation": "auto",
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 6,
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 16
        },
        "type": "timeseries",
        "title": "Quantum Advantage Ratio",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "postgres-datasource"
            },
            "rawSql": "WITH classical_avg AS (\n  SELECT \n    DATE_TRUNC('minute', timestamp) as time_bucket,\n    AVG(execution_time_ms) as classical_time\n  FROM jobs\n  WHERE solver_type = 'classical'\n    AND timestamp >= NOW() - INTERVAL '24 hours'\n  GROUP BY time_bucket\n),\nquantum_avg AS (\n  SELECT \n    DATE_TRUNC('minute', timestamp) as time_bucket,\n    AVG(execution_time_ms) as quantum_time\n  FROM jobs\n  WHERE solver_type = 'quantum'\n    AND timestamp >= NOW() - INTERVAL '24 hours'\n  GROUP BY time_bucket\n)\nSELECT \n  c.time_bucket as time,\n  CASE \n    WHEN q.quantum_time > 0 THEN c.classical_time / q.quantum_time\n    ELSE 1\n  END as quantum_advantage\nFROM classical_avg c\nJOIN quantum_avg q ON c.time_bucket = q.time_bucket\nORDER BY time;",
            "format": "time_series"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 0,
              "showPoints": "auto"
            },
            "unit": "none",
            "decimals": 2
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
      {
        "id": 7,
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 16
        },
        "type": "timeseries",
        "title": "Resource Utilization",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "postgres-datasource"
            },
            "rawSql": "SELECT \n  timestamp as time,\n  solver_type,\n  SUM(power_usage_w) as power_usage\nFROM jobs\nWHERE timestamp >= NOW() - INTERVAL '24 hours'\nGROUP BY timestamp, solver_type\nORDER BY timestamp;",
            "format": "time_series"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 1,
              "fillOpacity": 70,
              "showPoints": "never",
              "stacking": {
                "mode": "normal",
                "group": "A"
              }
            },
            "unit": "watt"
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
      {
        "id": 8,
        "gridPos": {
          "h": 10,
          "w": 24,
          "x": 0,
          "y": 24
        },
        "type": "table",
        "title": "Recent Jobs",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "postgres-datasource"
            },
            "rawSql": "SELECT \n  timestamp,\n  problem_type,\n  solver_type as decision,\n  execution_time_ms as time,\n  energy_consumed_j as energy\nFROM jobs\nORDER BY timestamp DESC\nLIMIT 20;",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "timestamp"
              },
              "properties": [
                {
                  "id": "custom.width",
                  "value": 180
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "time"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "ms"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "energy"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "joule"
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "footer": {
            "show": false
          }
        }
      }
    ]
  },
  "overwrite": true,
  "folderUid": "",
  "message": "QuantumEdge Pipeline Performance Dashboard"
}
